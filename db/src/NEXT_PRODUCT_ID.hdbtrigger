TRIGGER "NEXT_PRODUCT_ID_TRIG"
BEFORE INSERT ON "PRODUCTS" 
REFERENCING NEW ROW newProduct
FOR EACH ROW 
BEGIN
	DECLARE ERROR_COND CONDITION FOR SQL_ERROR_CODE 10001;
    DECLARE NEXT_ID INTEGER;
    IF :newProduct.ID IS NULL THEN
        SELECT "PRODUCT_ID".NEXTVAL ID into NEXT_ID FROM DUMMY;
        newProduct.ID = NEXT_ID;
    ELSEIF :newProduct.ID < 29999999 THEN
        SIGNAL ERROR_COND SET MESSAGE_TEXT = 'ID specified is out of range; should be > 29999999';  
    END IF;
END